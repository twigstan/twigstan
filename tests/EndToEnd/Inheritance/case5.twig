{% extends '@EndToEnd/Inheritance/layout.twig' %}
{% import '@EndToEnd/Inheritance/layout.twig' as layout %}

{% set title = 'Welcome' %}
{% set subtitle = 'Welcome' %}

{% block main %}
    <h1>{{ subtitle }}</h1>
    {% set name = 'Ruud' %}

    {% assert_type variable_set_after_header_block_before_main_block 'true' %}
    {% assert_type variable_set_after_main_block_before_footer_block '*ERROR*' %}
{% endblock %}

{% block footer %}
    <footer>Copyright</footer>
    {% assert_type variable_set_after_header_block_before_main_block 'true' %}
    {% assert_type variable_set_after_main_block_before_footer_block 'true' %}
{% endblock %}
